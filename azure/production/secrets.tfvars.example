# Secrets Template - Copy to secrets.tfvars and fill in your values
# DO NOT COMMIT secrets.tfvars to version control

# Azure Configuration
subscription_id = "YOUR_AZURE_SUBSCRIPTION_ID"

# Virtual Machine SSH Keys
# Add your SSH public keys here
vm_admin_ssh_keys = [
  "ssh-rsa AAAAB3NzaC1yc2EAAAA... your-ssh-public-key-here"
]
